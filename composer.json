    "keywords": ["laravel", "instagram", "api", "crawler"],
    "type": "library",
    "license" : "MIT",
    "authors": [
        {
            "name": "Amir Bashiri",
            "email": "bashiri.net@gmail.com"
        }
    ],
    "minimum-stability": "dev",
